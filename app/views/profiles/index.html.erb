<!-- Allows users to see a list of all other users -->
<h1>Profiles</h1>

<!-- Iterates through @profiles instance variable array and displays each profile name as a link to profile page -->
<ul>
	<% @profiles.each do |profile| %>
		<li>
			<strong><%= link_to profile.name + ' ' + profile.surname, profile_path(profile.id) %></strong>
		</li>
	<% end %>
</ul>